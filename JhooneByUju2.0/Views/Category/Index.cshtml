@model List<Category>

<h1>Store Categories</h1>
<br />

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Display Order
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var category in Model.OrderBy(c => c.DisplayOrder))
        {
            <tr>
                <td>@category.Name</td>
                <td> @category.DisplayOrder</td>
            </tr>
            
        }
    </tbody>
    
    


</table>